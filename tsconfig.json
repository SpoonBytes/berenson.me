{
	"compilerOptions": {
		"outDir": "./dist",
		"sourceMap": true,
		"noEmitOnError": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"alwaysStrict": true,
		"noFallthroughCasesInSwitch": true,
		"strict": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"target": "es5"
	}
}
